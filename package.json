{
  "name": "Soundness",
  "version": "1.0.0",
  "description": "be more productive with sounds",
  "dependencies": {
    "menubar": "*"
  },
  "devDependencies": {
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.36.0"
  },
  "main": "main.js",
  "scripts": {
    "build-mac": "electron-packager . Soundness --platform=darwin --arch=x64 --version=0.36.0 --icon=Icon.icns",
    "build-win": "electron-packager . Soundness --platform=win32 --arch=ia32 --version=0.36.0",
    "build-lin": "electron-packager . Soundness --platform=linux --arch=ia32 --version=0.36.0 --icon=Icon.icns",
    "start": "electron ."
  }
}
